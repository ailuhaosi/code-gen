import request from '@/utils/request'

export function get${className}Info(params) {
    return request({
        url: `/${moduleName}/${pathName}/info`,
        method: 'get',
        params
    })
}

export function get${className}List(params) {
    return request({
        url: '/${moduleName}/${pathName}/list',
        method: 'get',
        params
    })
}

export function save${className}(data) {
    return request({
        url: '/${moduleName}/${pathName}/save',
        method: 'post',
        data
    })
}

export function update${className}(data) {
    return request({
        url: '/${moduleName}/${pathName}/update',
        method: 'put',
        data
    })
}

export function delete${className}ByIds(data) {
    return request({
        url: `/${moduleName}/${pathName}/delete`,
        method: 'delete',
        data
    })
}
